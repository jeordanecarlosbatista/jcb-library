{
  "name": "@jeordanecarlosbatista/test",
  "version": "1.0.7",
  "description": "Test",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "eslint": "eslint \"{lib,test}/**/*.ts\" --fix",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:cov": "jest --watch --coverage",
    "test:cov": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@faker-js/faker": "^9.0.3",
    "@types/jest": "^29.5.8",
    "async": "^3.2.6",
    "async-await-retry": "^2.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.7.0",
    "typescript-transform-paths": "^3.5.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup-env.mjs"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "lib/**/*.{ts,js}"
    ],
    "coveragePathIgnorePatterns": [],
    "moduleNameMapper": {
      "^@test/(.*)$": "<rootDir>/test/$1",
      "^@lib/(.*)$": "<rootDir>/lib/$1"
    },
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "@jeordanecarlosbatista/jcb-aws-sqs": "^1.0.20"
  },
  "engines": {
    "node": "20.17.0"
  },
  "gitHead": "179bb2398849ae38c3027fd9cdc02f6501d7dce2"
}
